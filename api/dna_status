curl $RPC_HOST:$RPC_PORT -s -H "Content-Type: application/json" -X POST --data '{
                       "jsonrpc":"2.0",
                       "key":"5161aefcb6cb2b289686365bb1c46daa",
                       "id":1,
                       "method":"dna_identity",
                       "params": []
               }' | jq -r -c '(.result.online)' | grep 'true' &> /dev/null; if [ $? == 0 ]; then echo "ON"; else echo "OFF"; fi
