curl $RPC_HOST:$RPC_PORT -s -H "Content-Type: application/json" -X POST --data '{
                       "jsonrpc":"2.0",
                       "key":"$API_KEY",
                       "id":1,
                       "method":"dna_sendTransaction",
                       "params": [
				{
			"from": "$node_address",
		        "to": "$destination_address",
			"amount": "$transaction_amount"
			}
			]
               }' | jq -r -c '(.result)'
